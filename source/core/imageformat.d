struct ImageFormatInfo 
{
    ImageFormat format;
    ComponentLayout componentLayout;
    byte[4] componentBits; /* if [0] == [1] == [2] == [3] == 0, the format is a block format. */
    ImageFormatType formatType;
    @property auto isBlockFormat() const { return componentBits[0] == 0; }
    @property auto numChannels() const { return ComponentLayoutNumChannels[componentLayout]; } 
}


